#include<stdio.h>
int main(){
int a[10],p=3,b[10],count1=0,count2=0,i;
for(i=0;i<3;i++){
    scanf("%d",&a[i]);
}
for(i=0;i<3;i++){
    scanf("%d",&b[i]);
}
for(i=0;i<3;i++){
    if(a[i]>b[i]){
        count1++;
    }
    else if(a[i]<b[i]){
        count2++;
    }
}
printf("%d %d",count1,count2);

    return 0;
}
